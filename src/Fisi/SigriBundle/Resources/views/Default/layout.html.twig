{% extends 'FisiSigriBundle:Default:base.html.twig' %}
{% block stylesheets %}
    {{ parent() }}    



    <link rel="stylesheet" href="{{ asset('bundles/fisisigri/css/font/entypo.eot') }}" />

    <style>
        @font-face {
            font-family: 'entypo';
            src: url('{{asset('bundles/fisisigri/template/css/font-icons/entypo/font/entypo.eot?71205724')}}}');
            src: url('{{asset('bundles/fisisigri/template/css/font-icons/entypo/font/entypo.eot?71205724#iefix')}}') format('embedded-opentype'),
                url('{{asset('bundles/fisisigri/template/css/font-icons/entypo/font/entypo.woff')}}') format('woff'),
                url('{{asset('bundles/fisisigri/template/css/font-icons/entypo/font/entypo.ttf?71205724')}}') format('truetype'),
                url('{{asset('bundles/fisisigri/template/css/font-icons/entypo/font/entypo.svg?71205724#entypo')}}') format('svg');
            font-weight: normal;
            font-style: normal;
        }
    </style>

    {% stylesheets 
        '@FisiSigriBundle/Resources/public/template/js/jquery-ui/css/no-theme/jquery-ui-1.10.3.custom.min.css'
        '@FisiSigriBundle/Resources/public/template/css/font-icons/entypo/css/entypo.css'
        '@FisiSigriBundle/Resources/public/template/css/bootstrap.css'
        '@FisiSigriBundle/Resources/public/template/css/neon-core.css'
        '@FisiSigriBundle/Resources/public/template/css/neon-theme.css'
        '@FisiSigriBundle/Resources/public/template/css/neon-forms.css'
        '@FisiSigriBundle/Resources/public/template/js/selectboxit/jquery.selectBoxIt.css'
        
    %}
    <link href="{{ asset_url }}" type="text/css" rel="stylesheet" />
    {% endstylesheets %}
    <link href="http://fonts.googleapis.com/css?family=Noto+Sans:400,700,400italic" type="text/css" rel="stylesheet" />

{% endblock %}



{% block body %}
    <div class="page-container"><!-- add class "sidebar-collapsed" to close sidebar by default, "chat-visible" to make chat appear always -->
        <div class="container-fluid">
            <div class="row">
                {% block cabecera %}
                    <!-- Profile Info and Notifications -->
                    <div class="col-md-6 col-sm-8 clearfix">

                        <ul class="user-info pull-left pull-none-xsm">

                            <!-- Profile Info -->
                            <li class="profile-info dropdown"><!-- add class "pull-right" if you want to place this from right -->

                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                    {% image '@FisiSigriBundle/Resources/public/template/images/thumb-1@2x.png' %}
                                    <img src="{{ asset_url }}" alt="" class="img-circle" width="44" />
                                    {% endimage %}
                                    Mario Alvarez
                                </a>

                                <ul class="dropdown-menu">

                                    <!-- Reverse Caret -->
                                    <li class="caret"></li>

                                    <!-- Profile sub-links -->
                                    <li>
                                        <a href="extra-timeline.html">
                                            <i class="entypo-user"></i>
                                            Edit Profile
                                        </a>
                                    </li>

                                    <li>
                                        <a href="mailbox.html">
                                            <i class="entypo-mail"></i>
                                            Inbox
                                        </a>
                                    </li>

                                    <li>
                                        <a href="extra-calendar.html">
                                            <i class="entypo-calendar"></i>
                                            Calendar
                                        </a>
                                    </li>

                                    <li>
                                        <a href="#">
                                            <i class="entypo-clipboard"></i>
                                            Tasks
                                        </a>
                                    </li>
                                </ul>
                            </li>

                        </ul>



                    </div>


                    <!-- Raw Links -->
                    <div class="col-md-6 col-sm-4 clearfix hidden-xs">

                        <ul class="list-inline links-list pull-right">
                            <li>
                                <a href="#">
                                    Log Out <i class="entypo-logout right"></i>
                                </a>
                            </li>
                        </ul>

                    </div>



                {% endblock %}
            </div>
        </div>
       

        <div class="container-fluid" style="padding-top: 15px;">  
            {% block pagePrincipal %}
                {% endblock %}
            
            
            <div class="row">

                <div class="col-lg-2" style="padding-left: 2px; padding-right: 0px; border-top: solid; border-color: #f7f7f7; border-width: 1px;">

                    <div class="sidebar-menu" style="border-width:0px;">
                        <div class="sidebar-menu-inner">

                            {% block mainmenu %}            

                            {% endblock %}

                        </div>
                    </div>

                </div>


                <div class="col-lg-10" style="border-top: solid; border-left: solid; border-color: #f7f7f7; border-width: 1px; "> 
                    <div class="container-fluid">
                        <div clas="row">                  
                            <div class="main-content" style="float: right;">
                            {% block content %}
                            {% endblock %}
                        </div>
                    </div>
                    </div>
                </div>



            </div>



        </div>
    </div>

{% endblock %}

{% block javascripts %}
    {% javascripts 
        '@FisiSigriBundle/Resources/public/template/js/jquery-1.11.1.min.js'     
        '@FisiSigriBundle/Resources/public/template/js/gsap/main-gsap.js'
        '@FisiSigriBundle/Resources/public/template/js/jquery-ui/js/jquery-ui-1.10.3.minimal.min.js'
        '@FisiSigriBundle/Resources/public/template/js/bootstrap.js'
        '@FisiSigriBundle/Resources/public/template/js/joinable.js'
        '@FisiSigriBundle/Resources/public/template/js/resizeable.js'
        '@FisiSigriBundle/Resources/public/template/js/neon-api.js'
        '@FisiSigriBundle/Resources/public/template/js/jvectormap/jquery-jvectormap-1.2.2.min.js'
        '@FisiSigriBundle/Resources/public/template/js/jvectormap/jquery-jvectormap-europe-merc-en.js'
        '@FisiSigriBundle/Resources/public/template/js/jquery.sparkline.min.js'
        '@FisiSigriBundle/Resources/public/template/js/rickshaw/vendor/d3.v3.js'
        '@FisiSigriBundle/Resources/public/template/js/rickshaw/rickshaw.min.js'
        '@FisiSigriBundle/Resources/public/template/js/raphael-min.js'
        '@FisiSigriBundle/Resources/public/template/js/morris.min.js'
        '@FisiSigriBundle/Resources/public/template/js/toastr.js'
        '@FisiSigriBundle/Resources/public/template/js/neon-chat.js'
        '@FisiSigriBundle/Resources/public/template/js/neon-custom.js'
        '@FisiSigriBundle/Resources/public/template/js/bootstrap-datepicker.js'
        '@FisiSigriBundle/Resources/public/template/js/bootstrap-timepicker.min.js'        
        '@FisiSigriBundle/Resources/public/template/js/icheck/icheck.min.js'
        '@FisiSigriBundle/Resources/public/template/js/selectboxit/jquery.selectBoxIt.min.js'
         '@FisiSigriBundle/Resources/public/template/js/jquery.multi-select.js'
    %}
    


    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <script>$.noConflict();</script>
    
{% endblock %}