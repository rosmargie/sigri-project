{% extends 'FisiSigriBundle:Default:gestorLayout.html.twig' %}
{% block content %}   
  
    <script src="{{ asset('bundles/jquery/jquery-1.11.1.min.js') }}" type="text/javascript"></script>   

    <br/>
    <div class="row">
        <div class="col-lg-7">
            <div class="row">
                <fieldset >
                    <legend>Descripción
                        <div style="font-size: 45px; margin-top: -50px; ">
                            <a  class="tooltip-default" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Rennviar solicitud" onclick="jQuery('#dmensajeReenvioGestor').modal('show', {backdrop: 'static'});"  style="float: right;" >
                                <i class="entypo-mail" > </i> 
                            </a>                               
                        </div>  
                    </legend>
                    <div class="form-horizontal validate " style=" padding-left: 25px;  ">
                        <div class="form-group">
                            <div class="col-sm-3  control-label"  style="font-size: 12px; width: 130px;">ID solicitud</div>
                            <div class="col-sm-9">
                                <input id="m_idSolicitud" type="text"  value="{{solicitud.id_solicitud}}" disabled style="width: 15%; background-color: white; border: solid; border-color: #cccccc; border-width: 0.2px; padding-left: 5px; " >
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-3  control-label"  style="font-size: 12px; width: 130px;">Solicitante </div>
                            <div class="col-sm-9">
                                <input type="text" value="{{solicitud.empleado.getApellido_paterno()}} {{solicitud.empleado.getApellido_materno()}},  {{solicitud.empleado.getNombre()}}" disabled style="width: 100%; background-color: white; border: solid; border-color: #cccccc; border-width: 0.2px; padding-left: 5px;" >
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-3  control-label"  style="font-size: 12px; width: 130px;">Unidad orgánica</div>
                            <div class="col-sm-9">
                                <input type="text" value="{{solicitud.empleado.unidadOrganica.getNombre()}}" disabled style="width: 100%; background-color: white; border: solid; border-color: #cccccc; border-width: 0.2px; padding-left: 5px;">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-3  control-label"  style="font-size: 12px; width: 130px;">Dirección IP</div>
                            <div class="col-sm-9">
                                <input type="text" value="{{solicitud.ip}}" disabled style="width: 100%; background-color: white; border: solid; border-color: #cccccc; border-width: 0.2px; padding-left: 5px;">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-3  control-label"  style="font-size: 12px; width: 130px;">Titulo</div>
                            <div class="col-sm-9">
                                <input type="text" value="{{solicitud.titulo}}" disabled style="width: 100%; background-color: white; border: solid; border-color: #cccccc; border-width: 0.2px; padding-left: 5px;" >
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-3 control-label"  style="font-size: 12px; width: 130px;">Fecha de reporte </div>
                            <div class="col-sm-3">
                                <input type="text" value="{{solicitud.fecha_reporte|date("d/m/Y")}}" disabled style="width: 100%; background-color: white; border: solid; border-color: #cccccc; border-width: 0.2px; padding-left: 5px;" >
                            </div>
                            <div class="col-sm-3 control-label"  style="font-size: 12px; width: 130px; margin-left: 45px;">Hora de reporte </div>
                            <div class="col-sm-3">
                                <input type="text" value="{{solicitud.hora_reporte|date("h:i:s")}}" disabled style="width: 100%; background-color: white; border: solid; border-color: #cccccc; border-width: 0.2px; padding-left: 5px;" >
                                
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-3  control-label"  style="font-size: 12px; width: 130px;">Fecha de reserva </div>
                            <div class="col-sm-9">
                                <input type="date" value="{% if solicitud.fecha_reserva|date("Y") > 0 %}  {{solicitud.fecha_reserva|date("d/m/Y")}} {% endif %}
                                       " disabled style="width: 100%; background-color: white; height: 22px; border: solid; border-color: #cccccc; border-width: 0.2px; padding-left: 0px;"  >
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-3 control-label" style="font-size: 12px; width: 130px;">Descripción</div>
                            <div class="col-sm-9">
                                <textarea cols="68"  rows="12" disabled style="width: 100%; background-color: white; border: solid; border-color: #cccccc; border-width: 0.2px; resize: none; padding-left: 5px;">{{solicitud.descripcion}}</textarea>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label"  style="font-size: 12px; width: 130px;">Calificación</label>
                            <div class="col-sm-9">
