                                                    <input type="text" name="iCategoria"  id="tdCambiar{{solicitud.idsolicitud}}" value="{{solicitud.subcategoria|capitalize}}" disabled  style="background-color: white; width: 100%; border: solid; border-color: #cccccc; border-width: 1px; padding-left: 5px;" ></div>                                                                                                                                  
                                                {% endif %}
                                        </div> 
<br/> 
                                        <div class="row" style="margin: 25px 0px 20px 0px; text-align: center;">            
                                            {% if solicitud.subcategoria is null %}
                                                <a  class="btn" style="background-color: #44b4c4; color: white" onclick="jQuery('#dGuardarCambios').modal('show', {backdrop: 'static'});"  >
                                                    <i class="entypo-floppy"></i>
                                                    <span style="font-size: 12px;"> Guardar cambios</span>
                                                </a> 
<br/>												
                                        {% else %}{% endif %}
                                    </div>
                                </form>
                            </div>
                        {% else %} 
                            <table class="table table-bordered"    style="width: 90%;">

                                <tr><td style="background-color: #6666CC; color: white; text-align: left; ">
                                        <div class="col-sm-2  control-label" >Estado</div>
                                    </td>
                                    <td>
                                        <div class="col-sm-9">
                                            {{solicitud.estado|capitalize}}
                                        </div>   </td>                                                                         
                                </tr>
                                <tr><td style="background-color: #6666CC; color: white ">
                                        <div class="col-sm-2  control-label" >Tipo</div>
                                    </td><td>
                                        <div class="col-sm-9">
                                            {{solicitud.tipo|capitalize}}
                                        </div>   </td>                                                                         
                                </tr>
                                <tr><td style="background-color: #6666CC; color: white ">
                                        <div class="col-sm-2  control-label" >Prioridad</div>
                                    </td><td>
                                        <div class="col-sm-9">
                                            {{solicitud.prioridad|capitalize}}
                                        </div>   </td>                                                                         
                                </tr>
                                <tr><td style="background-color: #E; color: white ">
                                        <div class="col-sm-2  control-label" >Categoria</div>

                                    </td><td>
                                        <div class="col-sm-9">
                                            {{solicitud.categoria.nombrecategoria|capitalize}}

                                        </div>   </td>                                                                         
                                </tr>
                                <tr><td style="background-color: #6666CC; color: white ">
                                        <div class=" control-label" style="padding-left: 14px;">Sub-categoria</div>

                                    </td><td>
                                        <div class="col-sm-9">
                                            {{solicitud.subcategoria|capitalize}}
                                        </div>   </td>                                                                         
                                </tr>
                            </table>




                        {% endif %}</div>
                </div></div>
            
        </div>

    </div>

    <br/>
    <br/>
    <br/>

