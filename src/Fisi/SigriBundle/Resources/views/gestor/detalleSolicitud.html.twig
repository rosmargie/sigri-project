        <script>
                                jQuery("#btnRegistrarIncReq").click(function () {
                                    var idSolicitud = jQuery("#m_idSolicitud").val();
                                    var prioridad = jQuery("#m_slcPrioridad").val();
                                    var categoria = jQuery("#tdCambiar" + idSolicitud).val();
                                    var subCategoria = jQuery("#m_slcSubCategoria").val();


                                    jQuery.ajax({
                                        type: 'POST',
                                        url: '{{ path('registrarInicReq') }}',
                                        data: {
                                            'idSolicitud': idSolicitud,
                                            'prioridad': prioridad,
                                            'categoria': categoria,
                                            'subCategoria': subCategoria
                                        },
                                        dataType: 'json',
                                        success: function (data) {

                                            jQuery("#dGuardarCambios").modal('hide');
                                            jQuery("#formRegistrarReqInc").submit();
                                        }

                                    });

