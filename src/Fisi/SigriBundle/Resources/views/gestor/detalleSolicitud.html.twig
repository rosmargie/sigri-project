                                <i class="entypo-info-circled" style="color: #0000C0;"> Sin calificar </i>
                            </div>
                        </div>
                    </div>
                </fieldset>
            </div>

        </div>
        <div class="col-lg-1">
        </div>

        <div class="col-lg-4" >
            <div class="row">
                <div   style="border: solid; border-color: #00a651; border-width: 4px; border-radius: 20px; padding-top: 5px; padding-left: 9px; padding-right: 9px;">
                    <div class="row" style="padding-left: 22px;"><h3>Registro de incidencia/requerimiento</h3></div>
                    <div class="col-sm-4"></div>
                    <div class=" form-group col-sm-4 control-label"  style="font-size: 14px; color: green; text-align: center; margin-top: 5px;">Estado </div>
                    <div  class="form-group" style="padding: 20px 20px 0px 20px;">
                        <input type="text" value="{{solicitud.estado}}" disabled style="width: 100%;  background-color: white; border: solid; border-color: #cccccc; border-width: 0.2px; padding-left: 5px;">    
                    </div>
                    <div class="col-sm-4"></div>
                    <div class="form-group col-sm-4 control-label"  style="font-size: 14px; color: green; text-align: center;">Tipo </div>
                    <div  class="form-group" style="padding: 20px 20px 0px 20px;">
                        <input type="text" value="{{solicitud.tipo}}" disabled style="width: 100%; background-color: white; border: solid; border-color: #cccccc; border-width: 0.2px; padding-left: 5px;" >
                    </div>
                    {# Registro de incidencia/ requerimiento#}
                    <form role="form" id="formRegistrarReqInc" name="formRegistrarReqInc" method="post"  class="form-horizontal validate" action="{{path('detalle_solicitud_gestor',{'idSolicitud':solicitud.id_solicitud})}}" >                                
                        <div class="col-sm-4"></div>
                        {% if solicitud.prioridad == false %}
                            <div class="form-group col-sm-4 control-label "  style="font-size: 14px; text-align: center; margin-left: 0px; color: green; ">Prioridad  (*)</div>  
                            <div  class="form-group" style="padding: 20px 20px 0px 20px; ">
                                <select id="m_slcPrioridad" name="slcPrioridad" class="selectboxit-container selectboxit-container"  data-first-option="false" style=" width: 91%;  margin-left: 15px; padding-left: 5px; margin-top: 20px;">
                                    <option value="0" >--- Seleccionar --</option>
                                    <option value="ALTA">Alta</option>
                                    <option value="MEDIA" >Media</option>
                                    <option value="BAJA" >Baja</option>
                                </select>
                            </div>  
                        {% else %}    
                            <div class="form-group col-sm-4 control-label "  style="font-size: 14px; text-align: center; margin-left: 0px; color: green; ">Prioridad </div>  
                            <div  class="form-group" style="padding: 20px 20px 0px 20px; ">
                                <input type="text" value="{{solicitud.prioridad|capitalize}}" disabled style="width: 92%; background-color: white; border: solid; border-color: #cccccc; border-width: 0.2px; padding-left: 5px; margin-left: 13px; margin-top: 20px;" >
                            </div>  
                        {% endif %}
                        <div class="col-sm-4"></div> 
                        {% if solicitud.categoria.getNombre_categoria() == false %}
                            <div class="form-group col-sm-4 control-label "  style="font-size: 14px; text-align: center; margin-left: 0px; color: green;">Categoria (*)</div>
                             <div  class="form-group row" style="padding: 20px 20px 0px 20px; ">
                            <div class="col-sm-11"> 
                                <input type="text" name="iCategoria"  id="tdCambiar{{solicitud.id_solicitud}}" value="{{solicitud.categoria.getNombre_categoria()}}" disabled  style=" width: 100%; background-color: white; border: solid; border-color: #cccccc; border-width: 0.2px; padding-left: 5px; margin-top: 20px;" >
                            </div>
                            <div class="icon col-lg-1" style="font-size: 24px; margin-top: 10px; ">
                                <a  class="tooltip-default" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Consultar catálogo" onclick="jQuery('#dConsultarCatalogo').modal('show', {backdrop: 'static'});" >
                                    <i class="entypo-search" style="margin-left: -30px; color: green;"></i>
                                </a>
                            </div>  
                        </div>
                        {% else %}
                            <div class="form-group col-sm-4 control-label "  style="font-size: 14px; text-align: center; margin-left: 0px; color: green;">Categoria</div>
                            <div  class="form-group row" style="padding: 20px 20px 0px 15px; ">
                             <div class="col-sm-12"> 
                                <input type="text" name="iCategoria"  id="tdCambiar{{solicitud.id_solicitud}}" value="{{solicitud.categoria.getNombre_categoria()}}" disabled  style=" width: 100%; background-color: white; border: solid; border-color: #cccccc; border-width: 0.2px; padding-left: 5px; margin-top: 20px;" >
                            </div>
                            </div>
                        {% endif %}
                        <div class="col-sm-4"></div>
                        {% if solicitud.sub_categoria == false %}
                            <div class=" form-group col-sm-5"  style="font-size: 14px; text-align: center; margin-left: 0px; color: green;">Sub-categoria (*)</div>
                            <div  class="form-group" style="padding: 20px 20px 0px 20px;">
                                <select id="m_slcSubCategoria" name="slcSubCategoria" class="selectboxit-container selectboxit-container"  data-first-option="false" style=" width: 91%;  margin-left: 15px; padding-left: 5px; margin-top: 20px;">
                                    <option value="0">--- Seleccionar --</option>
                                    <option value="REQUERIMIENTO">Requerimiento</option>
                                    <option value="INCIDENCIA">Incidencia</option>
                                </select>
                            </div>
                        {% else %}
                            <div class=" form-group col-sm-5"  style="font-size: 14px; text-align: center; margin-left: 0px; color: green;">Sub-categoria</div>
                            <div  class="form-group" style="padding: 20px 20px 0px 20px;">
                                <input type="text" value="{{solicitud.sub_categoria|capitalize}}" disabled style="width: 92%; background-color: white; border: solid; border-color: #cccccc; border-width: 0.2px; padding-left: 5px; margin-left: 10px; margin-top: 20px; " >
                            </div>
                        {% endif %}

                        <div class="row" style="margin: 25px 0px 20px 0px; text-align: center;">            
                            <a  class="btn btn-success" onclick="jQuery('#dGuardarCambios').modal('show', {backdrop: 'static'});"  >
                                <i class="entypo-floppy"></i>
                                <span style="font-size: 12px;"> Guardar cambios</span>
                            </a>                
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>

    <div class="row"> 
        <ul class="nav nav-tabs bordered">
            {#   <li class="active">
                   <a href="#dArchivoRecepcion" data-toggle="tab">
                       <span class="visible-xs"><i class="entypo-home"></i></span>
