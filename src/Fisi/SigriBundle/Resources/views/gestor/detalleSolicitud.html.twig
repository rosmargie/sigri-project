                    <div class="modal-body row" style="padding: 20px 40px 40px 40px; ">
                        <div class="form-group">
                            <label class="col-lg-3"> Nombre de la categoria </label>
                            <div class="col-lg-5">
                                <input type="text" class="form-control" name="iNombreCategoriaV" data-validate="required,solicitante" data-message-required="Datos Obligatorios" placeholder="Nombre de la categoria a buscar">
                            </div>
                            <div class="col-lg-2">
                                <button type="button" class="btn btn-default entypo-search" style="width: 140px;"> Buscar </button>
                            </div>
                        </div>
                        <br/><br/>
                        <div>
                            <table class="table table-bordered datatable dataTable" id="tbListaCatalogo" role="grid" aria-describedby="table-1_info">
                                <thead>
                                    <tr>

                                        <th style="text-align: center;">Catagoria</th>
                                        <th style="text-align: center;">Descripción</th>
                                        <th style="text-align: center;">Cantidad</th>


                                    </tr>
                                </thead>
                                <tbody>
                                    {% for categoria in categorias %}
                                        <tr >
                                            <td>{{categoria.nombre_categoria}}</td>
                                            <td >{{categoria.descripcion_categoria}}</td>                                        

                                            <td> 


                                                <button  type="button" class="btn btn-info" data-dismiss="modal"  onclick="cambiar('{{solicitud.id_solicitud}}', '{{categoria.nombre_categoria}}');" >
