{% extends 'FisiSigriBundle:Default:gestorLayout.html.twig' %}
{% block content %}   

    <script src="{{ asset('bundles/jquery/jquery-1.11.1.min.js') }}" type="text/javascript"></script>   

    <br/>
    <div class="row">
        {% if  solicitud.categoria is null %}
            <div class="col-lg-7" >
            {% else %}
                <div class="col-lg-8" >        
                {% endif %}
                <div class="row" style="margin-left: 20px; background-color: #f7f4f4; border-radius: 10px;">
                    <br/>
                    <fieldset >
                        <legend style="margin-left: 10px;">Descripción                         
